<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.ed_p1_grupo07.CatalogoController">
   <children>
      <VBox fx:id="contenedor" alignment="TOP_CENTER" prefHeight="387.0" prefWidth="586.0" spacing="10.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="42.0" prefWidth="562.0" spacing="25.0">
               <children>
                  <ComboBox fx:id="CbxVehiculo" onAction="#filterVehiculo" prefWidth="150.0" />
                  <ComboBox fx:id="CbxOrder" onAction="#filterOrder" prefWidth="150.0" />
               </children>
            </HBox>
            <VBox fx:id="c" alignment="TOP_CENTER" prefHeight="269.0" prefWidth="586.0" spacing="15.0" />
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#atras" prefHeight="26.0" prefWidth="109.0" style="-fx-background-color: red;" text="Atras" textFill="WHITE" />
                  <Button mnemonicParsing="false" onAction="#verDetalle" prefHeight="26.0" prefWidth="107.0" style="-fx-background-color: red;" text="Ver mas detalle" textFill="WHITE" />
               </children>
            </HBox>
         </children>
         <padding>
            <Insets top="17.0" />
         </padding>
      </VBox>
   </children>
</AnchorPane>
